<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:ignore="HardcodedText">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/bt_equals"
            style="@style/ImeButton.Symbol"
            android:text="=" />

        <Button
            android:id="@+id/bt_underline"
            style="@style/ImeButton.Symbol"
            android:text="_" />

        <Button
            android:id="@+id/bt_less"
            style="@style/ImeButton.Symbol"
            android:text="&#60;" />

        <Button
            android:id="@+id/bt_more"
            style="@style/ImeButton.Symbol"
            android:text="&#62;" />

        <Button
            android:id="@+id/bt_bracket_square_left"
            style="@style/ImeButton.Symbol"
            android:text="[" />

        <Button
            android:id="@+id/bt_bracket_square_right"
            style="@style/ImeButton.Symbol"
            android:text="]" />

        <Button
            android:id="@+id/bt_1"
            style="@style/ImeButton.Symbol"
            android:text="1"/>

        <Button
            android:id="@+id/bt_2"
            style="@style/ImeButton.Symbol"
            android:text="2" />

        <Button
            android:id="@+id/bt_3"
            style="@style/ImeButton.Symbol"
            android:text="3"/>

        <Button
            android:id="@+id/bt_4"
            style="@style/ImeButton.Symbol"
            android:text="4" />

        <Button
            android:id="@+id/bt_5"
            style="@style/ImeButton.Symbol"
            android:text="5" />

        <Button
            android:id="@+id/bt_6"
            style="@style/ImeButton.Symbol"
            android:text="6" />

        <Button
            android:id="@+id/bt_7"
            style="@style/ImeButton.Symbol"
            android:text="7" />

        <Button
            android:id="@+id/bt_8"
            style="@style/ImeButton.Symbol"
            android:text="8" />

        <Button
            android:id="@+id/bt_9"
            style="@style/ImeButton.Symbol"
            android:text="9" />

        <Button
            android:id="@+id/bt_0"
            style="@style/ImeButton.Symbol"
            android:text="0" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/bt_dot"
            style="@style/ImeButton.Symbol"
            android:text="." />

        <Button
            android:id="@+id/bt_bracket_curly_left"
            style="@style/ImeButton.Symbol"
            android:text="{" />

        <Button
            android:id="@+id/bt_bracket_curly_right"
            style="@style/ImeButton.Symbol"
            android:text="}" />

        <Button
            android:id="@+id/bt_bracket_round_left"
            style="@style/ImeButton.Symbol"
            android:text="(" />

        <Button
            android:id="@+id/bt_bracket_round_right"
            style="@style/ImeButton.Symbol"
            android:text=")" />

        <Button
            android:id="@+id/bt_comma"
            style="@style/ImeButton.Symbol"
            android:text="," />

        <Button
            android:id="@+id/bt_q"
            style="@style/ImeButton.Letter"
            android:text="q" />

        <Button
            android:id="@+id/bt_w"
            style="@style/ImeButton.Letter"
            android:text="w" />

        <Button
            android:id="@+id/bt_e"
            style="@style/ImeButton.Letter"
            android:text="e" />

        <Button
            android:id="@+id/bt_r"
            style="@style/ImeButton.Letter"
            android:text="r" />

        <Button
            android:id="@+id/bt_t"
            style="@style/ImeButton.Letter"
            android:text="t" />

        <Button
            android:id="@+id/bt_y"
            style="@style/ImeButton.Letter"
            android:text="y" />

        <Button
            android:id="@+id/bt_u"
            style="@style/ImeButton.Letter"
            android:text="u" />

        <Button
            android:id="@+id/bt_i"
            style="@style/ImeButton.Letter"
            android:text="i" />

        <Button
            android:id="@+id/bt_o"
            style="@style/ImeButton.Letter"
            android:text="o" />

        <Button
            android:id="@+id/bt_p"
            style="@style/ImeButton.Letter"
            android:text="p" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/bt_pound"
            style="@style/ImeButton.Symbol"
            android:text="#"/>

        <Button
            android:id="@+id/bt_percent"
            style="@style/ImeButton.Symbol"
            android:text="%" />

        <Button
            android:id="@+id/bt_backslash"
            style="@style/ImeButton.Symbol"
            android:text="\\" />

        <Button
            android:id="@+id/bt_slash"
            style="@style/ImeButton.Symbol"
            android:text="/" />

        <Button
            android:id="@+id/bt_star"
            style="@style/ImeButton.Symbol"
            android:text="*" />

        <Button
            android:id="@+id/bt_minus"
            style="@style/ImeButton.Symbol"
            android:text="-" />

        <Button
            android:id="@+id/bt_plus"
            style="@style/ImeButton.Symbol"
            android:text="+" />

        <Button
            android:id="@+id/bt_a"
            style="@style/ImeButton.Letter"
            android:text="a" />

        <Button
            android:id="@+id/bt_s"
            style="@style/ImeButton.Letter"
            android:text="s" />

        <Button
            android:id="@+id/bt_d"
            style="@style/ImeButton.Letter"
            android:text="d" />

        <Button
            android:id="@+id/bt_f"
            style="@style/ImeButton.Letter"
            android:text="f" />

        <Button
            android:id="@+id/bt_g"
            style="@style/ImeButton.Letter"
            android:text="g" />

        <Button
            android:id="@+id/bt_h"
            style="@style/ImeButton.Letter"
            android:text="h" />

        <Button
            android:id="@+id/bt_j"
            style="@style/ImeButton.Letter"
            android:text="j" />

        <Button
            android:id="@+id/bt_k"
            style="@style/ImeButton.Letter"
            android:text="k" />

        <Button
            android:id="@+id/bt_l"
            style="@style/ImeButton.Letter"
            android:text="l" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/bt_shift_left"
            style="@style/ImeButton.Shift"
            android:text="&#8679;"/>

        <Button
            android:id="@+id/bt_dpad_up"
            style="@style/ImeButton.Command"
            android:text="&#8593;"
            android:tag="@string/command_up"/>

        <Button
            android:id="@+id/bt_question"
            style="@style/ImeButton.Symbol"
            android:text="\?" />

        <Button
            android:id="@+id/bt_colon"
            style="@style/ImeButton.Symbol"
            android:text=":" />

        <Button
            android:id="@+id/bt_exclamation"
            style="@style/ImeButton.Symbol"
            android:text="!" />

        <Button
            android:id="@+id/bt_or"
            style="@style/ImeButton.Symbol"
            android:text="|"/>

        <Button
            android:id="@+id/bt_ampersand"
            style="@style/ImeButton.Symbol"
            android:text="&#38;" />

        <Button
            android:id="@+id/bt_quotes"
            style="@style/ImeButton.Symbol"
            android:text="&#34;" />

        <Button
            android:id="@+id/bt_z"
            style="@style/ImeButton.Letter"
            android:text="z" />

        <Button
            android:id="@+id/bt_x"
            style="@style/ImeButton.Letter"
            android:text="x" />

        <Button
            android:id="@+id/bt_c"
            style="@style/ImeButton.Letter"
            android:text="c" />

        <Button
            android:id="@+id/bt_v"
            style="@style/ImeButton.Letter"
            android:text="v" />

        <Button
            android:id="@+id/bt_b"
            style="@style/ImeButton.Letter"
            android:text="b" />

        <Button
            android:id="@+id/bt_n"
            style="@style/ImeButton.Letter"
            android:text="n" />

        <Button
            android:id="@+id/bt_m"
            style="@style/ImeButton.Letter"
            android:text="m" />

        <Button
            android:id="@+id/bt_semicolon"
            style="@style/ImeButton.Symbol"
            android:text=";" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/bt_dpad_left"
            style="@style/ImeButton.Command"
            android:text="&#8592;"
            android:tag="@string/command_left"/>

        <Button
            android:id="@+id/bt_dpad_down"
            style="@style/ImeButton.Command"
            android:text="&#8595;"
            android:tag="@string/command_down"/>

        <Button
            android:id="@+id/bt_dpad_right"
            style="@style/ImeButton.Command"
            android:text="&#8594;"
            android:tag="@string/command_right"/>

        <Button
            android:id="@+id/bt_at"
            style="@style/ImeButton.Symbol"
            android:text="\@" />

        <Button
            android:id="@+id/bt_dollar"
            style="@style/ImeButton.Symbol"
            android:text="$" />

        <Button
            android:id="@+id/bt_grave"
            style="@style/ImeButton.Symbol"
            android:text="`" />

        <Button
            android:id="@+id/bt_tilda"
            style="@style/ImeButton.Symbol"
            android:text="~" />

        <Button
            android:id="@+id/bt_apostrophe"
            style="@style/ImeButton.Symbol"
            android:text="'" />

        <Button
            android:id="@+id/bt_circumflex"
            style="@style/ImeButton.Symbol"
            android:text="^" />

        <Button
            android:id="@+id/bt_space"
            style="@style/ImeButton.Spacer"
            android:text="space"
            android:layout_weight="2"
            android:tag=" "/>

        <Button
            android:id="@+id/bt_tab"
            style="@style/ImeButton.Spacer"
            android:text="tab"
            android:layout_weight="2"
            android:tag="\t"/>

        <Button
            android:id="@+id/bt_switch"
            style="@style/ImeButton"
            android:text="&#8646;" />

        <Button
            android:id="@+id/bt_enter"
            style="@style/ImeButton.Spacer"
            android:text="&#8629;"
            android:layout_weight="2"
            android:tag="\n"/>

        <Button
            android:id="@+id/bt_backspace"
            style="@style/ImeButton.Command"
            android:text="&#8602;"
            android:tag="@string/command_backspace"/>
    </LinearLayout>
</LinearLayout>